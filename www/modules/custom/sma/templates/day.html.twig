{#
/**
 * @file day.html.twig
 * Default theme implementation to present Day data.
 *
 * This template is used when viewing Day pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_day()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('day') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
